{
  "pyprland": {
    "plugins": ["scratchpads", "magnify", "shift_monitors", "expose"]
  },
  "scratchpads": {
    "kitty": {
      "lazy": true,
      "command": "kitty --class scratchpad",
      "margin": 50
    },
    "warp-terminal": {
      "lazy": true,
      "command": "warp-terminal",
      "margin": 50
    },
    "cava": {
      "lazy": true,
      "command": "kitty --class kitty --title cava cava"
    },
    "ranger": {
      "lazy": true,
      "command": "kitty --class scratchpad ranger",
      "margin": 50
    },
    "pavucontrol": {
      "lazy": true,
      "command": "pavucontrol",
      "margin": 50,
      "unfocus": "hide",
      "animation": "fromTop"
    },
    "google-chrome-stable": {
      "lazy": true,
      "command": "google-chrome-stable"
    },
    "chromium": {
      "lazy": true,
      "command": "chromium"
    },
    "thorium": {
      "lazy": true,
      "command": "thorium-browser"
    },
    "firefox": {
      "lazy": true,
      "command": "firefox"
    },
    "firefox-nightly": {
      "lazy": true,
      "command": "firefox-nightly"
    },
    "nautilus": {
      "lazy": true,
      "command": "nautilus"
    },
    "thunar": {
      "lazy": true,
      "command": "thunar"
    }
  }
}
